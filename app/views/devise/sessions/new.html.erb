





<%= form_for(resource, as: resource_name, url: session_path(resource_name) , :html=>{:id=>"msform"}) do |f| %>
<fieldset>
<h2 class="fs-title">Inicia Sesion</h2>
<h3 class="fs-subtitle">Para mayores privilegios</h3>
  <div class="field">
    
    <%= f.email_field :email, autofocus: true,placeholder: "Email" %>
  </div>

  <div class="field">
    
    <%= f.password_field :password, autocomplete: "off" ,placeholder: "Contraseña"%>
  </div>

  <% if devise_mapping.rememberable? -%>
    <div class="field">
      <%= f.check_box :remember_me  %>
      <%= f.label :remember_me ,"Recordar mis datos", :class=>"fs-subtitle" %>
    </div>
  <% end -%>

  <div class="actions">
    <%= f.submit "Iniciar sesión" , :class=>"next action-button"%>
  </div>

 </fieldset>
<% end %>

<%= render "devise/shared/links" %>
